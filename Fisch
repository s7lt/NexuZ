local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "NexuZ Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "NexuZTest"})

local plr = game.Players.LocalPlayer

_G.Key = "Test2323"
_G.KeyInput = "String"

OrionLib:MakeNotification({
	Name = "Logged in as "..plr.Name,
	Content = "Welcome to our new script "..plr.Name,
	Image = "rbxassetid://4483345998",
	Time = 5
})

local Tab = Window:MakeTab({
	Name = "NexuZ Fisch",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Test",
	Default = "Type The Key üîë",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({ 
	Name = "Verify ‚úÖ",
	Callback = function()
      	if _G.KeyInput == _G.Key then
            MakeScriptHub()
            CorrectKeyNotif()
        else
            InCorrectKeyNotif()
        end
  	end    
})


local Tab = Window:MakeTab({
	Name = "Infos üìò",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddLabel("Username : "..plr.Name)

function CorrectKeyNotif()
    OrionLib:MakeNotification({
        Name = "Loading Script...",
        Content = "Correct Key",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function InCorrectKeyNotif()
    OrionLib:MakeNotification({
        Name = "Wrong Key ‚ùå",
        Content = "You typed the Wrong Key ‚ùå",
        Image = "rbxassetid://4483345998",
        Time = 4
    })
end

function MakeScriptHub()

	local infMoneyEnabled = false


	local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

	local Window = OrionLib:MakeWindow({Name = "NexuZ Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "NexuZ Unlocked ‚úÖ"})

	local Tab = Window:MakeTab({
        Name = "Test",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

	Tab:AddToggle({
		Name = "Enable Inf Money üí∏",
		Default = false,
		Callback = function(Value)
			infMoneyEnabled = Value
		
			if infMoneyEnabled then
	
				spawn(function()
					while infMoneyEnabled do
	
						game:GetService("ReplicatedStorage").RewardEvents.GiveReward:FireServer("5")
						
	
						wait(0.1)
					end
				end)
			end
		end
	})
	

end
